{
  "name": "pixi-controller",
  "version": "1.1.0",
  "description": "Set of controls for Pixi.js to facilitate general handling of external events.",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "bundle": "lib/index.iife.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Novout/pixi-controller"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src/**/*.{js,ts,tsx} --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "keywords": [
    "factory",
    "pixi.js",
    "graphics",
    "rendering",
    "webgl"
  ],
  "files": [
    "lib",
    "package.json"
  ],
  "bugs": {
    "url": "https://github.com/Novout/pixi-controller/issues"
  },
  "homepage": "https://novout.github.io/pixi-controller/",
  "author": "Novout",
  "license": "MIT",
  "dependencies": {
    "nom-events": "^1.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/jsdom": "^16.2.6",
    "@types/node": "^14.14.22",
    "@types/rollup": "^0.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "pixi.js": "^5.3.8",
    "prettier": "^2.2.1",
    "rollup": "^2.38.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.3",
    "vitepress": "^0.12.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint"
    ]
  }
}
